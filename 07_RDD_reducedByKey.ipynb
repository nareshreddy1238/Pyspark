   "source": [
    "from pyspark import SparkContext\n",
    "\n",
    "sc = SparkContext(\"local\", \"reduceByKeyExample\")\n",
    "data = sc.parallelize([(1, 3), (2, 5), (1, 7), (2, 8), (3, 2)])\n",
    "sums = data.reduceByKey(lambda a, b: a + b)\n",
    "print(sums.collect())\n",
    "sc.stop()"
   ]
